<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--  扫描模式  -->
    <string-array name="ble_mode" tools:ignore="InconsistentArrays">
        <item>@string/ble_mode_balanced</item>
        <item>@string/ble_mode_low_latency</item>
        <item>@string/ble_mode_low_power</item>
    </string-array>

    <!--  回调类型  -->
    <string-array name="ble_callback_type" tools:ignore="InconsistentArrays">
        <item>@string/callback_type_all_matches</item>
    </string-array>

    <!--  名称过滤选项  -->
    <string-array name="name_filter_items">
        <item>@string/starts_name</item>
        <item>@string/full_name</item>
    </string-array>
    <string-array name="mac_filter_items">
        <item>@string/starts_address</item>
        <item>@string/full_address</item>
    </string-array>
    <string-array name="options">
        <item>@string/read_data</item>
        <item>@string/write_data</item>
        <item>@string/enable_notify</item>
    </string-array>
    <string-array name="write_data_types">
        <item>@string/string</item>
        <item>@string/hex</item>
    </string-array>
    <string-array name="device_options">
        <item>@string/connect_device</item>
        <item>@string/view_scan_record</item>
    </string-array>
    <string-array name="booleans">
        <item>@string/yes</item>
        <item>@string/no</item>
    </string-array>
</resources>