<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--  扫描模式  -->
    <string-array name="ble_mode" tools:ignore="InconsistentArrays">
        <item>@string/ble_mode_balanced</item>
        <item>@string/ble_mode_low_latency</item>
        <item>@string/ble_mode_low_power</item>
    </string-array>

    <!--  回调类型  -->
    <string-array name="ble_callback_type" tools:ignore="InconsistentArrays">
        <item>@string/callback_type_all_matches</item>
    </string-array>

    <!--  名称过滤选项  -->
    <string-array name="name_filter_items">
        <item>@string/starts_name</item>
        <item>@string/full_name</item>
    </string-array>
    <string-array name="mac_filter_items">
        <item>@string/starts_address</item>
        <item>@string/full_address</item>
    </string-array>
</resources>